{
  "build": {
    "builder": "python",
    "buildCommand": "pip install -r requirements.txt",
    "startCommand": "python main.py",
    "watch": ["main.py", "modules/", "requirements.txt", "config.py", "database.py", "webserver.py"]
  },
  "deploy": {
    "healthcheckPath": "/",
    "teardown": true,
    "serverless": false
  },
  "env": {
    "DISCORD_BOT_TOKEN": "<YOUR_BOT_TOKEN_HERE>",
    "DB_PATH": "ticket_bot.db"
  },
  "networking": {
    "public": true,
    "tcpProxy": false
  }
}
